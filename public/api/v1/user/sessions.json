{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "/",
  "resourcePath": "sessions",
  "apis": [
    {
      "path": "/v1/user/sessions.json",
      "operations": [
        {
          "summary": "Sign in To OnShop",
          "notes": "      Signing in should give you `authentication_token`.<br />\n      You should add `authentication_token` to all upgoing requests\n      in header to authenticate your self!<br />\n      Header should be like this:</br>\n      <b>X-User-Token</b> 1G8_s7P-V-4MGojaKD7a<br />\n      <br />\n",
          "parameters": [
            {
              "paramType": "header",
              "name": "X-APP-Token",
              "type": "string",
              "description": "App Authentication Token",
              "required": true
            },
            {
              "paramType": "form",
              "name": "user[email]",
              "type": "email",
              "description": "User Email",
              "required": true
            },
            {
              "paramType": "form",
              "name": "user[password]",
              "type": "password",
              "description": "User password",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Ok"
            },
            {
              "code": 401,
              "responseModel": null,
              "message": "Unauthorized"
            },
            {
              "code": 422,
              "responseModel": null,
              "message": "Unprocessable Entity"
            }
          ],
          "nickname": "Api::V1::User::Sessions#create",
          "method": "post"
        }
      ]
    },
    {
      "path": "/v1/user/sessions/{id}.json",
      "operations": [
        {
          "summary": "Sign out",
          "parameters": [
            {
              "paramType": "header",
              "name": "X-APP-Token",
              "type": "string",
              "description": "App Authentication Token",
              "required": true
            },
            {
              "paramType": "header",
              "name": "X-User-Token",
              "type": "string",
              "description": "User Authentication Token",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Ok"
            },
            {
              "code": 401,
              "responseModel": null,
              "message": "Unauthorized"
            }
          ],
          "nickname": "Api::V1::User::Sessions#destroy",
          "method": "delete"
        }
      ]
    }
  ],
  "authorizations": null
}